<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/constraintLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.PlaceholderFragment">

    <Button
        android:id="@+id/button_TypicalCellsStoredHighMinus"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/MinusButton"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView_TypicalCellsStoredHigh"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredHigh" />

    <Button
        android:id="@+id/button_TypicalCellsStoredLowMinus"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/MinusButton"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@+id/textView_TypicalCellsStoredLow"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredLow" />

    <Button
        android:id="@+id/button_PreloadNo"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_NumberOfPreload"
        app:layout_constraintTop_toBottomOf="@+id/textview_NumberOfPreload" />

    <Button
        android:id="@+id/button_TypicalCellsStoredHighPlus"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlusButton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView_TypicalCellsStoredHigh"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredHigh" />

    <Button
        android:id="@+id/button_TypicalCellsStoredLowPlus"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/PlusButton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/textView_TypicalCellsStoredLow"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredLow" />

    <Button
        android:id="@+id/button_PreloadYes"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/textview_NumberOfPreload"
        app:layout_constraintTop_toBottomOf="@+id/textview_NumberOfPreload" />

    <Button
        android:id="@+id/button_TypicalCellsStoredHighCounter"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:text="@string/ZeroText"
        android:textColorLink="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_TypicalCellsStoredHighPlus"
        app:layout_constraintStart_toEndOf="@+id/button_TypicalCellsStoredHighMinus"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredHigh" />

    <Button
        android:id="@+id/button_TypicalCellsStoredLowCounter"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:background="@android:color/transparent"
        android:text="@string/ZeroText"
        android:textColorLink="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/button_TypicalCellsStoredLowPlus"
        app:layout_constraintStart_toEndOf="@+id/button_TypicalCellsStoredLowMinus"
        app:layout_constraintTop_toBottomOf="@+id/textView_TypicalCellsStoredLow" />

    <Button
        android:id="@+id/button_didNotMove"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_MoveBonus"
        app:layout_constraintTop_toBottomOf="@+id/textview_MoveBonus" />

    <Button
        android:id="@+id/button_PickupNo"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_Pickup"
        app:layout_constraintTop_toBottomOf="@+id/textview_Pickup" />

    <Button
        android:id="@+id/button_WorkingAutoNo"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/NoButton"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="@+id/workingAuto"
        app:layout_constraintTop_toBottomOf="@+id/workingAuto" />

    <Button
        android:id="@+id/button_PickupYes"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/textview_Pickup"
        app:layout_constraintTop_toBottomOf="@+id/textview_Pickup" />

    <Button
        android:id="@+id/button_WorkingAutoYes"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/workingAuto"
        app:layout_constraintTop_toBottomOf="@+id/workingAuto" />

    <Button
        android:id="@+id/button_moveBonusYes"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/YesButton"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="@+id/textview_MoveBonus"
        app:layout_constraintTop_toBottomOf="@+id/textview_MoveBonus" />

    <TextView
        android:id="@+id/textview_Pickup"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="@string/PickupText"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="@+id/button_PreloadYes"
        app:layout_constraintTop_toBottomOf="@+id/button_PreloadYes" />

    <TextView
        android:id="@+id/workingAuto"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="@string/WorkingAuto"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textview_MoveBonus"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="300dp"
        android:text="@string/MoveBonusText"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toEndOf="@+id/workingAuto"
        app:layout_constraintTop_toTopOf="@+id/workingAuto" />

    <TextView
        android:id="@+id/textview_NumberOfPreload"
        android:layout_width="144dp"
        android:layout_height="wrap_content"
        android:text="@string/NumberOfPreloads"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="@+id/button_moveBonusYes"
        app:layout_constraintTop_toTopOf="@+id/textview_StartPosition" />

    <!--       android:src="@drawable/field_image_for_scouting" -->

    <TextView
        android:id="@+id/textView_TypicalCellsStoredHigh"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="200dp"
        android:text="@string/TypicalCargoStoredHigh"

        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_avgCargoScored"
        app:layout_constraintTop_toBottomOf="@+id/textview_avgCargoScored" />

    <TextView
        android:id="@+id/textView_TypicalCellsStoredLow"
        android:layout_width="208dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"

        android:text="@string/TypicalCargoStoredLow"
        android:textAlignment="center"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="@+id/textview_avgCargoScored"
        app:layout_constraintTop_toTopOf="@+id/textView_TypicalCellsStoredHigh" />

    <Spinner
        android:id="@+id/spinner_StartPosition"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:spinnerMode="dropdown"
        app:layout_constraintStart_toStartOf="@+id/textview_StartPosition"
        app:layout_constraintTop_toBottomOf="@+id/textview_StartPosition" />

    <Spinner
        android:id="@+id/spinner_HumanPlayer"
        android:layout_width="208dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:spinnerMode="dropdown"
        app:layout_constraintStart_toStartOf="@+id/textview_HumanPlayer"
        app:layout_constraintTop_toBottomOf="@+id/textview_HumanPlayer" />

    <TextView
        android:id="@+id/textView_AutoSummary"
        android:layout_width="250dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="@string/AutoSummary"
        android:textAlignment="textStart"
        android:textSize="32sp"
        app:layout_constraintStart_toStartOf="@+id/button_TypicalCellsStoredHighMinus"
        app:layout_constraintTop_toBottomOf="@+id/spinner_HumanPlayer" />

    <EditText
        android:id="@+id/editText_autoSummary"
        android:layout_width="400dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:ems="10"
        android:gravity="start|top"
        android:hint="Type Here"
        android:inputType="textMultiLine"
        app:layout_constraintStart_toStartOf="@+id/textView_AutoSummary"
        app:layout_constraintTop_toBottomOf="@+id/textView_AutoSummary" />

    <TextView
        android:id="@+id/textview_avgCargoScored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:text="@string/AvgCargoScored"
        android:textSize="32sp"
        app:layout_constraintStart_toEndOf="@+id/textview_MoveBonus"
        app:layout_constraintTop_toTopOf="@+id/textview_MoveBonus" />

    <TextView
        android:id="@+id/textview_StartPosition"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:text="@string/StartPositionPit"
        android:textSize="31sp"
        app:layout_constraintStart_toStartOf="@+id/button_WorkingAutoYes"
        app:layout_constraintTop_toBottomOf="@+id/button_WorkingAutoYes" />

    <TextView
        android:id="@+id/textview_HumanPlayer"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="36dp"
        android:text="@string/HumanPlayer"
        android:textSize="32sp"
        app:layout_constraintEnd_toStartOf="@+id/button_TypicalCellsStoredLowMinus"
        app:layout_constraintStart_toEndOf="@+id/button_TypicalCellsStoredHighPlus"
        app:layout_constraintTop_toBottomOf="@+id/button_TypicalCellsStoredHighCounter" />

    <View
        android:id="@+id/divider3"
        android:layout_width="5dp"
        android:layout_height="798dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toStartOf="@+id/button_PreloadYes"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/divider4"
        android:layout_width="5dp"
        android:layout_height="798dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintEnd_toStartOf="@+id/button_TypicalCellsStoredHighMinus"
        app:layout_constraintStart_toEndOf="@+id/button_PreloadNo"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="309dp"
        android:layout_height="231dp"
        android:layout_marginTop="24dp"
        android:src="@drawable/betterbluefield2022"
        app:layout_constraintStart_toStartOf="@+id/spinner_StartPosition"
        app:layout_constraintTop_toBottomOf="@+id/spinner_StartPosition" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="101dp"
        android:text="@string/one"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="121dp"
        android:text="@string/two"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="97dp"
        android:layout_marginEnd="121dp"
        android:text="@string/three"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="144dp"
        android:layout_marginEnd="101dp"
        android:text="@string/four"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="161dp"
        android:layout_marginEnd="77dp"
        android:text="@string/five"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

    <TextView
        android:id="@+id/textView36"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="161dp"
        android:layout_marginEnd="48dp"
        android:text="@string/six"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7" />

</android.support.constraint.ConstraintLayout>