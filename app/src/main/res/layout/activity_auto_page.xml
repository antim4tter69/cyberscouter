<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AutoPage">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="287dp"
        android:layout_height="351dp"
        android:layout_marginTop="64dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/g"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="42dp" />

    <Button
        android:id="@+id/button_previous"
        android:layout_width="226dp"
        android:layout_height="95dp"
        android:layout_marginBottom="28dp"
        android:text="@string/PreviousButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.031"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_l1Center"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/LevelOneStartingPositionCenterButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/button_l1Left"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/button_l1Left"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:layout_marginStart="95dp"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="8dp"
        android:text="@string/LevelOneStartingPositionLeftButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/button_grndLeft"
        android:layout_width="198dp"
        android:layout_height="77dp"
        android:layout_marginTop="12dp"
        android:text="@string/GroundStartingPositionLeftButton"
        android:textSize="20sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.413"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/button_grndRight"
        android:layout_width="200dp"
        android:layout_height="77dp"
        android:layout_marginTop="8dp"
        android:text="@string/GroundStartingPositionRightButton"
        android:textSize="20sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.414"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_l1Left" />

    <Button
        android:id="@+id/button_moveBonusNo"
        android:layout_width="93dp"
        android:layout_height="95dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/NoButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/button_moveBonusYes"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/button19"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:layout_marginStart="95dp"
        android:layout_marginTop="8dp"
        android:text="@string/LevelTwoStartingPositionLeftButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/button_l1Left" />

    <Button
        android:id="@+id/button_moveBonusYes"
        android:layout_width="93dp"
        android:layout_height="95dp"
        android:layout_marginTop="8dp"
        android:text="@string/YesButton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.789"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <Button
        android:id="@+id/button_cargo"
        android:layout_width="199dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/PreloadCargoButton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/button_panel"
        android:layout_width="199dp"
        android:layout_height="64dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/PreloadPanelButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/button_cargo"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <Button
        android:id="@+id/button_none"
        android:layout_width="399dp"
        android:layout_height="64dp"
        android:layout_marginTop="8dp"
        android:text="@string/PreloadNoneButton"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_panel" />

    <Button
        android:id="@+id/button_l1Right"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="@string/LevelOneStartingPositionRightButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/button_l1Center"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <Button
        android:id="@+id/button_l2Right"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:layout_marginStart="109dp"
        android:layout_marginTop="8dp"
        android:text="@string/LevelTwoStartingPositionRightButton"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/button19"
        app:layout_constraintTop_toBottomOf="@+id/button_l1Right" />

    <Button
        android:id="@+id/button_skipMatch"
        android:layout_width="125dp"
        android:layout_height="95dp"
        android:layout_marginBottom="24dp"
        android:text="@string/SkipMatchButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.246"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_startMatch"
        android:layout_width="379dp"
        android:layout_height="201dp"
        android:layout_marginBottom="28dp"
        android:text="@string/StartMatchButton"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="@string/CurrentMatch"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.443"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="4dp"
        android:text="@string/CurrentTeam"
        android:textColor="@color/CurrentTeamColor"
        android:textColorLink="@color/CurrentTeamColor"
        android:textSize="30sp"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="542dp"
        android:layout_marginLeft="542dp"
        android:layout_marginTop="16dp"
        android:text="@string/title_activity_auto_page"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginLeft="95dp"
        android:layout_marginTop="147dp"
        android:text="@string/StartingPositionText"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/MoveBonusText"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.875"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="@string/PreloadText"
        android:textColor="@android:color/black"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button_moveBonusYes" />

    <Button
        android:id="@+id/button2"
        android:layout_width="173dp"
        android:layout_height="95dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="24dp"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/button_startMatch"
        app:layout_constraintStart_toEndOf="@+id/button_skipMatch"
        tools:text="@string/ResetButton" />

</android.support.constraint.ConstraintLayout>